{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "This is my first Chrome extension.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "My Extension"
  },
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ]
}
